{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/login.html"
    },
    {
      "source": "/login",
      "destination": "/public/login.html"
    },
    {
      "source": "/dashboard",
      "destination": "/public/index.html"
    },
    {
      "source": "/legacy/v2.8",
      "destination": "/public/legacy-v2.8.html"
    },
    {
      "source": "/legacy/v3.0",
      "destination": "/public/legacy-v3.0.html"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/public/assets/$1"
    }
  ],
  "headers": [
    {
      "source": "/assets/js/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/css/(.*).css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
